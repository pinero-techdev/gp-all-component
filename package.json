{
  "name": "gp-all-component",
  "version": "0.0.0",
  "scripts": {
    "ng": "ngw",
    "dev": "ngw serve tester --watch --port=4400",
    "start": "ngw serve tester --proxy-config proxy.conf.json",
    "build": "ngw build gp-all-component",
    "---- BUILD & PACK ----": "",
    "postbuild": "",
    "npm:pack": "cd dist/gp-all-component && npm pack",
    "package": "npm run build && npm run npm:pack",
    "deploy-component": "node deploy-gp-all-component.js",
    "---- TESTING ----": "",
    "test-lib": "ngw t gp-all-component",
    "test": "ngw t tester --watch=true",
    "test:prepush": "ngw t tester",
    "e2e": "cypress open",
    "---- LINTERS ----": "",
    "lint": "ngw lint --format stylish",
    "lint:dev": "ngw lint --format stylish --tslintConfig=\"tslint.json\"",
    "lint:prod": "ngw lint --format stylish --tslintConfig=\"tslint.prod.json\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:fix": "pretty-quick --staged",
    "fix:all": "npm run format:fix && npm run styles:fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "~7.2.0",
