{
  "name": "gp-all-component",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve tester --watch --port=4400",
    "start": "ng serve tester --proxy-config proxy.conf.json",
    "start:aot": "ng serve tester --proxy-config proxy.conf.json --aot",
    "build": "ng build gp-all-component",
    "---- DOCUMENTATION ----": "",
    "compodoc:generate": "npx compodoc -p projects/gp-all-component/tsconfig.lib.json",
    "compodoc:serve": "npx compodoc -s projects/gp-all-component/tsconfig.lib.json",
    "---- CONCAT FILES ----": "",
    "concat:json:result": "node ./scripts/concat-json -o ../projects/gp-all-component/src/lib/resources/i18n/i18n.json",
    "concat:json": "glob \"./projects/gp-all-component/src/lib/resources/i18n/**/*.json\" | npm run concat:json:result",
    "---- BUILD & PACK ----": "",
    "postbuild": "",
    "npm:pack": "cd dist/gp-all-component && npm pack",
    "package": "npm run build && npm run npm:pack",
    "deploy-component": "node deploy-gp-all-component.js",
    "---- TESTING ----": "",
    "test-lib": "ng t gp-all-component --watch=true",
    "test": "ng t tester --watch=true",
    "test:prod": "ng t gp-all-component --watch=false",
    "e2e": "cypress open",
    "---- LINTERS ----": "",
    "lint": "ng lint --format stylish",
    "lint:dev": "ng lint --format stylish --tslintConfig=\"tslint.json\"",
    "lint:prod": "ng lint --format stylish --tslintConfig=\"tslint.prod.json\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:fix": "pretty-quick --staged",
    "fix:all": "npm run format:fix && npm run styles:fix",
    "cy:run": "cypress run",
    "---- COMMIT HOOKS ----": ""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "^7.1.0",
    "quill": "^1.3.6",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.4.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^3.0.2",
    "@types/node": "~8.9.4",
    "@types/webpack": "4.4.26",
    "chance": "1.0.18",
    "cli-glob": "^0.1.0",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "css-loader": "^2.1.1",
    "cypress": "3.2.0",
    "fs-extra": "^7.0.1",
    "husky": "1.3.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-typescript": "^4.0.0",
    "merge-jsons-webpack-plugin": "1.0.18",
    "merge-webpack-plugin": "0.2.1",
    "moment": "^2.24.0",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "4.1.5",
    "path": "^0.12.7",
    "prettier": "1.16.4",
    "pretty-quick": "1.10.0",
    "scss-bundle": "^2.5.0",
    "style-loader": "^0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "3.5.4",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.3.2",
    "typescript": "3.2.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.1"
  }
}
