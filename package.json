{
  "name": "gp-all-component",
  "version": "14.0.7-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve tester --watch --port=4400",
    "start": "ng serve tester --proxy-config proxy.conf.json",
    "start:aot": "ng serve tester --proxy-config proxy.conf.json --aot",
    "---- DOCUMENTATION ----": "",
    "compodoc:generate": "npx compodoc -p projects/gp-all-component/tsconfig.lib.json",
    "compodoc:serve": "npx compodoc -s projects/gp-all-component/tsconfig.lib.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "---- GUIDELINES ----": "",
    "guide:dev": "vuepress dev guidelines",
    "guide:build": "vuepress build guidelines",
    "---- BUILD & PACK ----": "",
    "bundle:scss": "ts-node ./scripts/css-bundle.ts",
    "build": "ng build gp-all-component && npm run bundle:scss",
    "build:watch": "concurrently \"ng build gp-all-component --watch\" \"wait-on --delay 5000 file:dist/gp-all-component/lib/resources && npm run bundle:scss\"",
    "postbuild": "npm run npm:pack",
    "npm:pack": "cd dist/gp-all-component && npm pack",
    "deploy-component": "node ./scripts/deploy-gp-all-component.js",
    "---- BUILD RELEASES ----": "",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor --git-tag-version=false",
    "release:major": "npm version major --git-tag-version=false",
    "release:patch:lib": "cd projects/gp-all-component/ && npm version patch --git-tag-version=false",
    "release:minor:lib": "cd projects/gp-all-component/ && npm version minor --git-tag-version=false",
    "release:major:lib": "cd projects/gp-all-component/ && npm version major --git-tag-version=false",
    "---- PRE-BUILDS ----": "",
    "prebuild:patch": "run-s release:patch release:patch:lib build",
    "prebuild:minor": "run-s release:minor release:minor:lib build",
    "prebuild:major": "run-s release:major release:major:lib build",
    "---- TESTING ----": "",
    "test:lib": "ng t gp-all-component --watch=true",
    "test:prod": "ng t gp-all-component --watch=false",
    "e2e": "cypress open",
    "---- LINTERS ----": "",
    "lint": "ng lint --format stylish",
    "lint:dev": "ng lint --format stylish --tslintConfig=\"tslint.json\"",
    "lint:prod": "ng lint --format stylish --tslintConfig=\"tslint.prod.json\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:fix": "pretty-quick --staged",
    "fix:all": "npm run format:fix && npm run styles:fix",
    "cy:run": "cypress run",
    "---- COMMIT HOOKS ----": ""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.12",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.12",
    "@angular/compiler": "^14.2.12",
    "@angular/core": "^14.2.12",
    "@angular/forms": "^14.2.12",
    "@angular/platform-browser": "^14.2.12",
    "@angular/platform-browser-dynamic": "^14.2.12",
    "@angular/router": "^14.2.12",
    "@fullcalendar/core": "^5.11.0",
    "chart.js": "^3.8.0",
    "core-js": "^2.5.4",
    "fontawesome": "^5.6.3",
    "hammerjs": "^2.0.8",
    "npm": "^6.9.0",
    "postcss-preset-env": "^7.7.2",
    "primeflex": "3.2.1",
    "primeicons": "5.0.0",
    "primeng": "14.1.1",
    "property-watch-decorator": "^1.2.5",
    "quill": "^1.3.6",
    "rxjs": "^6.5.2",
    "util": "^0.12.4",
    "webpack": "^5.0.0",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=12.14.0",
    "npm": ">=6.4.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.2",
    "@angular/cli": "^14.2.3",
    "@angular/compiler-cli": "^14.2.12",
    "@angular/language-service": "^14.2.12",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@compodoc/compodoc": "^1.1.9",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^3.0.2",
    "@types/node": "~12.0.0",
    "@types/webpack": "5.28.0",
    "chance": "1.0.18",
    "cli-glob": "^0.1.0",
    "codelyzer": "^6.0.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.6.0",
    "cypress": "^12.3.0",
    "file-saver": "^2.0.2",
    "fs-extra": "^7.0.1",
    "grunt-contrib-requirejs": "^1.0.0",
    "jasmine": "^3.4.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-typescript": "^5.0.1",
    "moment": "^2.24.0",
    "ng-packagr": "^14.2.1",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "2.8.3",
    "pretty-quick": "1.10.0",
    "prompt-password": "^1.2.0",
    "scp2": "^0.5.0",
    "scss-bundle": "^3.1.2",
    "style-loader": "^0.23.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier": "9.0.0",
    "stylelint-config-standard": "29.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "4.3.0",
    "tsickle": "^0.46.3",
    "tslib": "^2.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.13.10",
    "typescript": "4.7.4",
    "vuepress": "^0.14.11",
    "wait-on": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s fix:all lint:dev"
    }
  }
}
