{
  "name": "gp-all-component",
  "version": "1.5.1",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve tester --watch --port=4400",
    "start": "ng serve tester --proxy-config proxy.conf.json",
    "start:aot": "ng serve tester --proxy-config proxy.conf.json --aot",
    "---- DOCUMENTATION ----": "",
    "compodoc:generate": "npx compodoc -p projects/gp-all-component/tsconfig.lib.json",
    "compodoc:serve": "npx compodoc -s projects/gp-all-component/tsconfig.lib.json",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "---- GUIDELINES ----": "",
    "guide:dev": "vuepress dev guidelines",
    "guide:build": "vuepress build guidelines",
    "---- BUILD & PACK ----": "",
    "bundle:scss": "ts-node ./scripts/css-bundle.ts",
    "build": "ng build gp-all-component && npm run bundle:scss",
    "build:watch": "concurrently \"ng build gp-all-component --watch\" \"wait-on --delay 5000 file:dist/gp-all-component/lib/resources && npm run bundle:scss\"",
    "postbuild": "npm run npm:pack",
    "npm:pack": "cd dist/gp-all-component && npm pack",
    "deploy-component": "node ./scripts/deploy-gp-all-component.js",
    "---- BUILD RELEASES ----": "",
    "release:patch": "npm version patch",
    "release:minor": "npm version minor --git-tag-version=false",
    "release:major": "npm version major --git-tag-version=false",
    "release:patch:lib": "cd projects/gp-all-component/ && npm version patch --git-tag-version=false",
    "release:minor:lib": "cd projects/gp-all-component/ && npm version minor --git-tag-version=false",
    "release:major:lib": "cd projects/gp-all-component/ && npm version major --git-tag-version=false",
    "---- PRE-BUILDS ----": "",
    "prebuild:patch": "run-s release:patch release:patch:lib build",
    "prebuild:minor": "run-s release:minor release:minor:lib build",
    "prebuild:major": "run-s release:major release:major:lib build",
    "---- TESTING ----": "",
    "test:lib": "ng t gp-all-component --watch=true",
    "test:prod": "ng t gp-all-component --watch=false",
    "e2e": "cypress open",
    "---- LINTERS ----": "",
    "lint": "ng lint --format stylish",
    "lint:dev": "ng lint --format stylish --tslintConfig=\"tslint.json\"",
    "lint:prod": "ng lint --format stylish --tslintConfig=\"tslint.prod.json\"",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "styles:fix": "stylelint \"src/**/*.scss\" --fix",
    "stylelint-check": "stylelint-config-prettier-check",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "format:fix": "pretty-quick --staged",
    "fix:all": "npm run format:fix && npm run styles:fix",
    "cy:run": "cypress run",
    "---- COMMIT HOOKS ----": "",
    "precommit": "run-s fix:all lint:dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.11",
    "@angular/cdk": "^7.3.6",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "inputmask": "^5.0.7",
    "node-sass": "^6.0.1",
    "npm": "^6.9.0",
    "primeflex": "^1.0.0-rc.1",
    "primeicons": "^1.0.0",
    "primeng": "7.1.3",
    "property-watch-decorator": "^1.2.5",
    "quill": "^1.3.6",
    "rxjs": "^6.6.7",
    "webpack": "^4.31.0",
    "zone.js": "~0.8.26"
  },
  "engines": {
    "node": ">=10.15.3",
    "npm": ">=6.4.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@compodoc/compodoc": "^1.1.9",
    "@types/bluebird": "3.5.34",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/karma": "^3.0.2",
    "@types/node": "~12.0.0",
    "@types/webpack": "4.4.26",
    "chance": "1.0.18",
    "cli-glob": "^0.1.0",
    "codelyzer": "~4.5.0",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.1",
    "cypress": "3.2.0",
    "file-saver": "^2.0.2",
    "fs-extra": "^7.0.1",
    "husky": "1.3.1",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-typescript": "^4.0.0",
    "moment": "^2.24.0",
    "ng-packagr": "~4.4.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "prettier": "1.16.4",
    "pretty-quick": "^1.10.0",
    "prompt-password": "^1.2.0",
    "scp2": "^0.5.0",
    "scss-bundle": "^2.5.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "5.0.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-scss": "3.5.4",
    "ts-node": "^7.0.1",
    "tsickle": "0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-etc": "1.3.2",
    "typescript": ">3.1.1 <3.3",
    "vuepress": "^0.14.11",
    "wait-on": "^3.2.0"
  }
}
